<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SaveServ</servlet-name>
        <servlet-class>p1.SaveServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewServ</servlet-name>
        <servlet-class>p1.ViewServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteServ</servlet-name>
        <servlet-class>p1.DeleteServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateServ</servlet-name>
        <servlet-class>p1.UpdateServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateServ2</servlet-name>
        <servlet-class>p1.UpdateServ2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SaveServ</servlet-name>
        <url-pattern>/SaveServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewServ</servlet-name>
        <url-pattern>/ViewServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteServ</servlet-name>
        <url-pattern>/DeleteServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateServ</servlet-name>
        <url-pattern>/UpdateServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateServ2</servlet-name>
        <url-pattern>/UpdateServ2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
